<%= form_for :project, url: projects_path do |f| %>
  <p>
    project name <br>
    <%= f.text_field(:name, {:class => 'form-control'}) %>
  </p>
  <p>
    project creator <br>
    <%= f.text_field(:creator, {:class => 'form-control'}) %>
  </p>
  <p>
    project priority <br>
    <%= f.text_field(:priority, {:class => 'form-control'}) %>
  </p>
  <%= f.submit %>
<% end %>
